{
  "name": "portfolio",
  "version": "1.0.0-alpha.0",
  "description": "My Portfolio website",
  "author": "Kamontat Chantrachirathumrong",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "clean": "rimraf .nuxt dist",
    "clean:all": "rimraf .nuxt dist yarn.lock node_modules",
    "start-server": "yarn build && yarn start",
    "start-test": "cypress run --reporter mocha-multi-reporters --reporter-options configFile=cypress/config/mocha.json",
    "cypress": "cypress",
    "cypress:run": "cypress run",
    "cypress:ui": "cypress open",
    "test": "start-server-and-test start-server http://localhost:3000 start-test",
    "test:ci": "yarn test --record"
  },
  "dependencies": {
    "@nuxtjs/axios": "5.0.0",
    "@nuxtjs/pwa": "2.6.0",
    "nuxt": "2.3.4",
    "nuxt-i18n": "5.4.4",
    "nuxt-purgecss": "0.0.2"
  },
  "devDependencies": {
    "autoprefixer": "8.6.4",
    "babel-eslint": "8.2.6",
    "babel-polyfill": "6.26.0",
    "cypress": "3.1.3",
    "eslint": "5.9.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "5.0.0",
    "jsdom": "13.0.0",
    "mocha": "5.2.0",
    "mocha-junit-reporter": "1.18.0",
    "mocha-multi-reporters": "1.1.7",
    "mochawesome": "^3.1.1",
    "node-sass": "4.10.0",
    "nyc": "13.1.0",
    "prettier": "1.14.3",
    "rimraf": "2.6.2",
    "sass-loader": "7.1.0",
    "start-server-and-test": "1.7.11",
    "tailwindcss": "0.6.6"
  }
}
